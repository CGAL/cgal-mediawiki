        "mediawiki/semantic-media-wiki": "^4.1",
        "mediawiki/page-forms": "^5.7",
        "mediawiki/semantic-forms-select": "4.0.0-beta",
        "mediawiki/semantic-watchlist": "^1.3",
        "data-values/data-values": "^3.1",
